{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/dist/main.js"
    },
    {
      "src": "/users/(.*)?",
      "dest": "backend/dist/main.js"
    },
    {
      "src": "/auth/(.*)",
      "dest": "backend/dist/main.js"
    }
  ]
}
